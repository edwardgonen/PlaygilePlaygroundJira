                                progressLabel.text(parseInt(progressbar.progressbar("value")) + "%");
                            },
                            complete: function () {
                                progressLabel.text("All data loaded!");
                                setTimeout(() => { progressbar.hide() }, 3000)
                            }
                        });
                    });
